[project]
	description = This is my team's repo
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/John Doe (jdoe123)
[access "refs/for/refs/*"]
	read = group user/John Doe (jdoe123)
	push = group user/John Doe (jdoe123)
	label-Code-Review = -2..+2 group user/John Doe (jdoe123)
	label-Verified = -1..+1 group user/John Doe (jdoe123)
	rebase = group user/John Doe (jdoe123)
	removeReviewer = group user/John Doe (jdoe123)
	submit = group user/John Doe (jdoe123)
[access "refs/heads/*"]
	read = group user/John Doe (jdoe123)
	create = group user/John Doe (jdoe123)
	push = +force group user/John Doe (jdoe123)
	pushMerge = group user/John Doe (jdoe123)
[access "refs/tags/*"]
	push = +force group user/John Doe (jdoe123)
	pushTag = group user/John Doe (jdoe123)
	pushSignedTag = group user/John Doe (jdoe123)
